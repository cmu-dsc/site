---
import "@fontsource/jetbrains-mono/400.css";
import "@fontsource/jetbrains-mono/700.css";
import { Image } from "astro:assets";
import chromaLogo from "../assets/sponsors/agents/chroma.svg";
import deepmindLogo from "../assets/sponsors/agents/deepmind.png";
import googleCloudLogo from "../assets/sponsors/agents/google-cloud.svg";
import langchainLogo from "../assets/sponsors/agents/langchain.png";
import lindyLogo from "../assets/sponsors/agents/lindy.webp";
import llamaIndexLogo from "../assets/sponsors/agents/llamaindex.svg";
import mistralLogo from "../assets/sponsors/agents/mistral.png";
import modalLogo from "../assets/sponsors/agents/modal.svg";
import replicateLogo from "../assets/sponsors/agents/replicate.svg";
import perplexityLogo from "../assets/sponsors/agents/perplexity.png";
import exaLogo from "../assets/sponsors/agents/exa.png";
import LinkedinIcon from "../icons/linkedin.svg?raw";
import TwitterIcon from "../icons/twitter.svg?raw";
import WebsiteIcon from "../icons/website.svg?raw";
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>AI Agents Weekend | CMU Data Science Club</title>
    <meta
      name="description"
      content="Join us for AI Agents Weekend at Carnegie Mellon University. Learn about the latest developments in autonomous AI agents, LLM applications, and multi-agent systems from industry experts and researchers."
    />
    <script
      is:inline
      defer
      src="https://cloud.umami.is/script.js"
      data-website-id="7899076f-1eca-4c1e-bb90-db959ce41077"></script>
  </head>
  <body>
    <main
      class="min-h-screen bg-black text-white font-['JetBrains_Mono'] leading-[20px]"
    >
      <a
        href="/"
        class="fixed top-[40px] left-[40px] text-white hover:text-gray-400 transition-colors"
        aria-label="Back to home"
      >
        ← back
      </a>

      <div
        class="h-full max-w-4xl mx-auto p-[40px] pt-[80px] flex flex-col gap-[60px]"
      >
        <!-- Hero Section -->
        <div class="flex-none space-y-[20px]">
          <div class="text-sm text-gray-400 h-[20px]">
            CMU DATA SCIENCE CLUB PRESENTS
          </div>
          <h1 class="text-4xl font-bold leading-[40px] h-[40px]">
            AI AGENTS WEEKEND
          </h1>
          <div class="text-xl text-blue-400 h-[20px]">APRIL 12-13, 2025</div>
          <div class="text-gray-400 h-[20px]">POSNER HALL A35</div>
          <p
            class="text-gray-400 max-w-2xl border-l-2 border-gray-700 pl-4 my-[20px] leading-[20px]"
          >
            Join us for a weekend exploring autonomous AI agents. Compete for
            $4,000 in cash prizes, plus credits and swag for winners. All
            participants receive $750 in credits. Work in teams of up to 4.
          </p>
          <p class="text-gray-500 italic text-sm max-w-2xl leading-[20px]">
            For research talks and posters, check out the <a
              href="https://cmu-agent-workshop.github.io/"
              class="text-blue-400 hover:text-blue-300">CMU Agent Workshop</a
            > on April 10-11.
          </p>
          <a
            href="https://lu.ma/tzy817vr"
            class="inline-block bg-white text-black px-[20px] py-[10px] hover:bg-gray-200 transition-colors h-[40px]"
            data-umami-event="Agents Weekend Registration Click"
          >
            REGISTER →
          </a>
        </div>

        <!-- Schedule and Sponsors Sections -->
        <div class="flex flex-col gap-[40px]">
          <!-- Schedule Section -->
          <div class="space-y-[40px]">
            <h2
              class="text-2xl border-b border-gray-700 pb-[10px] leading-[30px] h-[30px]"
            >
              SCHEDULE
            </h2>
            <div class="text-gray-400 space-y-[20px]">
              {/* Friday */}
              <details class="group">
                <summary
                  class="text-white mb-[10px] cursor-pointer hover:text-gray-300 transition-colors list-none flex items-center gap-2"
                >
                  <span
                    class="inline-block w-4 text-gray-400 transform group-open:rotate-90 transition-transform"
                    >▶</span
                  >
                  FRIDAY, APRIL 11
                </summary>
                <div
                  class="space-y-[10px] pl-8 border-l border-gray-700 mt-[10px]"
                >
                  <div>
                    5:00 PM - 6:00 PM · <a
                      href="https://lu.ma/7sedh8ch"
                      class="text-blue-400 hover:text-blue-300"
                      >LLMs 101 Workshop & Team Matching Mixer</a
                    >
                  </div>
                </div>
              </details>
              {/* Saturday */}
              <details class="group">
                <summary
                  class="text-white mb-[10px] cursor-pointer hover:text-gray-300 transition-colors list-none flex items-center gap-2"
                >
                  <span
                    class="inline-block w-4 text-gray-400 transform group-open:rotate-90 transition-transform"
                    >▶</span
                  >
                  SATURDAY, APRIL 12
                </summary>
                <div
                  class="space-y-[10px] pl-8 border-l border-gray-700 mt-[10px]"
                >
                  <div>
                    2:00 PM - 3:00 PM · <span class="text-white">Check-in</span>
                  </div>
                  <div>
                    2:30 PM - 3:00 PM · <span class="text-white"
                      >DeepMind Keynote: AI Studio & Gemini</span
                    >
                    <div
                      class="pl-4 mt-1 text-sm bg-gray-900 bg-opacity-30 p-2 rounded"
                    >
                      <div class="flex items-center gap-2">
                        <span class="text-blue-300 font-semibold"
                          >Paige Bailey</span
                        >
                        <span class="text-gray-500"
                          >DevEx Engineering Lead @ DeepMind</span
                        >
                        <div class="flex items-center gap-1 ml-auto">
                          <a
                            href="https://x.com/DynamicWebPaige"
                            target="_blank"
                            rel="noopener noreferrer"
                            class="text-blue-400 hover:text-blue-300"
                            title="Twitter"
                          >
                            <div class="h-4 w-4" set:html={TwitterIcon} />
                          </a>
                          <a
                            href="https://www.linkedin.com/in/dynamicwebpaige/"
                            target="_blank"
                            rel="noopener noreferrer"
                            class="text-blue-400 hover:text-blue-300"
                            title="LinkedIn"
                          >
                            <div class="h-4 w-4" set:html={LinkedinIcon} />
                          </a>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="text-blue-400">3:00 PM · Hacking Begins</div>
                  <div>
                    3:00 PM - 3:30 PM · <span class="text-white"
                      >Deepmind Tutorial</span
                    >
                    <div
                      class="pl-4 mt-1 text-sm bg-gray-900 bg-opacity-30 p-2 rounded"
                    >
                      <div class="flex items-center gap-2">
                        <span class="text-blue-300 font-semibold"
                          >Paige Bailey</span
                        >
                        <span class="text-gray-500"
                          >DevEx Engineering Lead @ DeepMind</span
                        >
                        <div class="flex items-center gap-1 ml-auto">
                          <a
                            href="https://x.com/DynamicWebPaige"
                            target="_blank"
                            rel="noopener noreferrer"
                            class="text-blue-400 hover:text-blue-300"
                            title="Twitter"
                          >
                            <div class="h-4 w-4" set:html={TwitterIcon} />
                          </a>
                          <a
                            href="https://www.linkedin.com/in/dynamicwebpaige/"
                            target="_blank"
                            rel="noopener noreferrer"
                            class="text-blue-400 hover:text-blue-300"
                            title="LinkedIn"
                          >
                            <div class="h-4 w-4" set:html={LinkedinIcon} />
                          </a>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div>
                    3:35 PM - 4:00 PM · <span class="text-white"
                      >LlamaIndex Tutorial</span
                    >
                    <div
                      class="pl-4 mt-1 text-sm bg-gray-900 bg-opacity-30 p-2 rounded"
                    >
                      <div class="flex items-center gap-2">
                        <span class="text-blue-300 font-semibold"
                          >Laurie Voss</span
                        >
                        <span class="text-gray-500"
                          >VP Developer Relations @ LlamaIndex</span
                        >
                        <div class="flex items-center gap-1 ml-auto">
                          <a
                            href="https://seldo.com/"
                            target="_blank"
                            rel="noopener noreferrer"
                            class="text-blue-400 hover:text-blue-300"
                            title="Website"
                          >
                            <div class="h-4 w-4" set:html={WebsiteIcon} />
                          </a>
                          <a
                            href="https://www.linkedin.com/in/seldo/"
                            target="_blank"
                            rel="noopener noreferrer"
                            class="text-blue-400 hover:text-blue-300"
                            title="LinkedIn"
                          >
                            <div class="h-4 w-4" set:html={LinkedinIcon} />
                          </a>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div>
                    4:05 PM - 4:30 PM · <span class="text-white"
                      >Replicate Tutorial</span
                    >
                    <div
                      class="pl-4 mt-1 text-sm bg-gray-900 bg-opacity-30 p-2 rounded"
                    >
                      <div class="flex items-center gap-2">
                        <span class="text-blue-300 font-semibold"
                          >Pranav Sekhar</span
                        >
                        <span class="text-gray-500">Growth @ Replicate</span>
                        <div class="flex items-center gap-1 ml-auto">
                          <a
                            href="https://www.linkedin.com/in/pranavsekhar/"
                            target="_blank"
                            rel="noopener noreferrer"
                            class="text-blue-400 hover:text-blue-300"
                            title="LinkedIn"
                          >
                            <div class="h-4 w-4" set:html={LinkedinIcon} />
                          </a>
                        </div>
                      </div>
                      <div class="flex items-center gap-2 mt-2">
                        <span class="text-blue-300 font-semibold">Dan Buch</span
                        >
                        <span class="text-gray-500"
                          >Staff Software Engineer @ Replicate</span
                        >
                        <div class="flex items-center gap-1 ml-auto">
                          <a
                            href="https://www.linkedin.com/in/dbuch/"
                            target="_blank"
                            rel="noopener noreferrer"
                            class="text-blue-400 hover:text-blue-300"
                            title="LinkedIn"
                          >
                            <div class="h-4 w-4" set:html={LinkedinIcon} />
                          </a>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div>
                    4:35 PM - 5:00 PM · <span class="text-white"
                      >Exa Tutorial</span
                    >
                    <div
                      class="pl-4 mt-1 text-sm bg-gray-900 bg-opacity-30 p-2 rounded"
                    >
                      <div class="flex items-center gap-2">
                        <span class="text-blue-300 font-semibold"
                          >Ishan Goswami</span
                        >
                        <span class="text-gray-500"
                          >Software Engineer @ Exa</span
                        >
                        <div class="flex items-center gap-1 ml-auto">
                          <a
                            href="https://www.linkedin.com/in/theishangoswami/"
                            target="_blank"
                            rel="noopener noreferrer"
                            class="text-blue-400 hover:text-blue-300"
                            title="LinkedIn"
                          >
                            <div class="h-4 w-4" set:html={LinkedinIcon} />
                          </a>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div>
                    5:00 PM - 6:00 PM · <span class="text-white"
                      >Sponsor Tabling</span>
                  </div>
                  <div>
                    6:00 PM - 7:00 PM · <span class="text-white">Dinner</span>
                  </div>
                  <div>
                    6:00 PM - 7:00 PM · <span class="text-white"
                      >Replicate Product & Startup Talk</span
                    >
                    <div
                      class="pl-4 mt-1 text-sm bg-gray-900 bg-opacity-30 p-2 rounded"
                    >
                      <div class="flex items-center gap-2">
                        <span class="text-blue-300 font-semibold"
                          >Pranav Sekhar</span
                        >
                        <span class="text-gray-500">Growth @ Replicate</span>
                        <div class="flex items-center gap-1 ml-auto">
                          <a
                            href="https://www.linkedin.com/in/pranavsekhar/"
                            target="_blank"
                            rel="noopener noreferrer"
                            class="text-blue-400 hover:text-blue-300"
                            title="LinkedIn"
                          >
                            <div class="h-4 w-4" set:html={LinkedinIcon} />
                          </a>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </details>

              {/* Sunday */}
              <details class="group">
                <summary
                  class="text-white mb-[10px] cursor-pointer hover:text-gray-300 transition-colors list-none flex items-none gap-2"
                >
                  <span
                    class="inline-block w-4 text-gray-400 transform group-open:rotate-90 transition-transform"
                    >▶</span
                  >
                  SUNDAY, APRIL 13
                </summary>
                <div
                  class="space-y-[10px] pl-8 border-l border-gray-700 mt-[10px]"
                >
                  <div>
                    3:00 PM - 4:00 PM · <span class="text-white"
                      >Project Presentations</span>
                  </div>
                  <div>
                    4:00 PM - 4:30 PM · <span class="text-white"
                      >Final Presentations</span>
                  </div>
                  <div>
                    4:30 PM - 5:00 PM · <span class="text-white">Judging</span>
                  </div>
                  <div>
                    5:00 PM - 6:00 PM · <span class="text-white"
                      >Prize Ceremony</span>
                  </div>
                </div>
              </details>
              {/* CMU Agent Workshop */}
              <div class="h-[40px]">
                <a
                  href="https://cmu-agent-workshop.github.io/"
                  class="hover:text-gray-300 text-l text-white"
                >
                  CMU AGENT 2025 WORKSHOP EVENTS
                </a>
              </div>
            </div>
          </div>

          <!-- Sponsors Section -->
          <div class="space-y-[20px]">
            <h2
              class="text-2xl border-b border-gray-700 pb-[10px] leading-[30px] h-[30px]"
            >
              TITLE SPONSOR
            </h2>
            <div class="border border-gray-700">
              <a
                href="https://deepmind.google/"
                target="_blank"
                rel="noopener noreferrer"
                class="flex items-center justify-center p-[30px] hover:bg-gray-900 transition-colors"
              >
                <Image
                  src={deepmindLogo}
                  alt="DeepMind"
                  class="w-1/2 h-auto object-contain"
                  width={500}
                  height={200}
                />
              </a>
            </div>

            <div class="border border-gray-700 mt-[20px]">
              <a
                href="https://cloud.google.com/"
                target="_blank"
                rel="noopener noreferrer"
                class="flex items-center justify-center p-[30px] hover:bg-gray-900 transition-colors"
              >
                <Image
                  src={googleCloudLogo}
                  alt="Google Cloud"
                  class="w-1/2 h-auto object-contain"
                  width={500}
                  height={200}
                />
              </a>
            </div>

            <h2
              class="text-2xl border-b border-gray-700 pb-[10px] leading-[30px] h-[30px]"
            >
              SUPPORTED BY
            </h2>
            <div
              class="grid grid-cols-2 md:grid-cols-3 gap-0 border border-gray-700"
            >
              <a
                href="https://www.llamaindex.ai"
                target="_blank"
                rel="noopener noreferrer"
                class="flex items-center justify-center p-[20px] border border-gray-700 hover:bg-gray-900 transition-colors"
              >
                <Image
                  src={llamaIndexLogo}
                  alt="LlamaIndex"
                  class="h-12 w-auto object-contain"
                  width={400}
                  height={160}
                />
              </a>
              <a
                href="https://replicate.com"
                target="_blank"
                rel="noopener noreferrer"
                class="flex items-center justify-center p-[20px] border border-gray-700 hover:bg-gray-900 transition-colors"
              >
                <Image
                  src={replicateLogo}
                  alt="Replicate"
                  class="h-12 w-auto object-contain brightness-0 invert"
                  width={400}
                  height={160}
                />
              </a>
              <a
                href="https://modal.com"
                target="_blank"
                rel="noopener noreferrer"
                class="flex items-center justify-center p-[20px] border border-gray-700 hover:bg-gray-900 transition-colors"
              >
                <Image
                  src={modalLogo}
                  alt="Modal"
                  class="h-12 w-auto object-contain brightness-0 invert"
                  width={400}
                  height={160}
                />
              </a>
              <a
                href="https://exa.ai/"
                target="_blank"
                rel="noopener noreferrer"
                class="flex items-center justify-center p-[20px] border border-gray-700 hover:bg-gray-900 transition-colors"
              >
                <Image
                  src={exaLogo}
                  alt="Exa"
                  class="h-12 w-auto object-contain brightness-0 invert"
                  width={400}
                  height={160}
                />
              </a>
              <a
                href="https://mistral.ai"
                target="_blank"
                rel="noopener noreferrer"
                class="flex items-center justify-center p-[20px] border border-gray-700 hover:bg-gray-900 transition-colors"
              >
                <Image
                  src={mistralLogo}
                  alt="Mistral"
                  class="h-12 w-auto object-contain"
                  width={400}
                  height={160}
                />
              </a>
              <a
                href="https://www.langchain.com"
                target="_blank"
                rel="noopener noreferrer"
                class="flex items-center justify-center p-[20px] border border-gray-700 hover:bg-gray-900 transition-colors"
              >
                <Image
                  src={langchainLogo}
                  alt="LangChain"
                  class="h-12 w-auto object-contain"
                  width={400}
                  height={160}
                />
              </a>
              <a
                href="https://www.lindy.ai"
                target="_blank"
                rel="noopener noreferrer"
                class="flex items-center justify-center p-[20px] border border-gray-700 hover:bg-gray-900 transition-colors"
              >
                <Image
                  src={lindyLogo}
                  alt="Lindy"
                  class="h-12 w-auto object-contain"
                  width={400}
                  height={160}
                />
              </a>
              <!-- <a
                href="https://weaviate.io"
                target="_blank"
                rel="noopener noreferrer"
                class="flex items-center justify-center p-[20px] border border-gray-700 hover:bg-gray-900 transition-colors"
              >
                <Image
                  src={weaviateLogo}
                  alt="Weaviate"
                  class="h-12 w-auto object-contain"
                  width={400}
                  height={160}
                />
              </a> -->
              <a
                href="https://trychroma.com"
                target="_blank"
                rel="noopener noreferrer"
                class="flex items-center justify-center p-[20px] border border-gray-700 hover:bg-gray-900 transition-colors"
              >
                <Image
                  src={chromaLogo}
                  alt="Chroma"
                  class="h-12 w-auto object-contain"
                  width={400}
                  height={160}
                />
              </a>

              <a
                href="https://www.perplexity.ai/discover"
                target="_blank"
                rel="noopener noreferrer"
                class="flex items-center justify-center p-[20px] border border-gray-700 hover:bg-gray-900 transition-colors"
              >
                <Image
                  src={perplexityLogo}
                  alt="Perplexity"
                  class="h-12 w-auto object-contain"
                  width={400}
                  height={160}
                />
              </a>
            </div>
          </div>
        </div>

        <!-- Footer -->
        <div
          class="flex-none pt-[20px] border-t border-gray-700 text-gray-400 text-sm"
        >
          <p class="leading-[20px]">CARNEGIE MELLON UNIVERSITY</p>
          <p class="leading-[20px]">PITTSBURGH, PA</p>
        </div>
      </div>
    </main>
  </body>
</html>

<style is:global>
  @tailwind base;
  @tailwind components;
  @tailwind utilities;
</style>
